<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.formelement {
		display: block;
		margin: 5px
	}	

	</ui:style>
	<b.gwt:FormPanel ui:field="formPanel">
		<b.gwt:FlowPanel ui:field="flowPanel">
			<g:FileUpload ui:field="fileUpload" styleName="{style.formelement}" />
			<g:Hidden ui:field="templateName" />
			<g:Hidden ui:field="templateLanguage" />			
			<b:SubmitButton ui:field="submitButton" type="PRIMARY"  marginRight="5" marginLeft="5"/>
			<b:Button ui:field="cancelButton" />
		</b.gwt:FlowPanel>		
	</b.gwt:FormPanel>
</ui:UiBinder> 